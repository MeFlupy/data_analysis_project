      "source": [
        "# Лабораторная работа №1. Арифметика, ввод-вывод\n",
        "# Вариант 6: n = 240 минут\n",
        "\n",
        "n = 240  # количество минут с начала суток\n",
        "\n",
        "# Учитываем только минуты в пределах одних суток\n",
        "n = n % 1440\n",
        "\n",
        "# Вычисляем часы и минуты\n",
        "hours = n // 60\n",
        "minutes = n % 60\n",
        "\n",
        "# Вывод результата\n",
        "print(hours, minutes)\n"
      ]
